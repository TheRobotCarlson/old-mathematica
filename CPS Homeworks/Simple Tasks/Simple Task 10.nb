Cell[BoxData[
 RowBox[{
  RowBox[{"rectanglesToFun2", "[", 
   RowBox[{"fun_", ",", 
    RowBox[{"{", 
     RowBox[{"lower_", ",", "upper_"}], "}"}], ",", "p_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"c", "=", 
       FractionBox[
        RowBox[{"(", 
         RowBox[{"upper", "-", "lower"}], ")"}], "p"]}], ",", 
      RowBox[{"root", "=", 
       RowBox[{"x", "/.", 
        RowBox[{"FindRoot", "[", 
         RowBox[{
          RowBox[{"fun", "[", "x", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "lower"}], "}"}]}], "]"}]}]}]}], "}"}], ",", 
    RowBox[{"(*", 
     RowBox[{
      RowBox[{"creates", " ", "the", " ", "increment"}], ",", " ", 
      RowBox[{"finds", " ", "the", " ", "root"}]}], "*)"}], 
    RowBox[{"Show", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Plot", "[", 
        RowBox[{
         RowBox[{"fun", "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"lower", "-", 
            RowBox[{"2", "c"}]}], ",", 
           RowBox[{"upper", "+", 
            RowBox[{"2", "c"}]}]}], "}"}], ",", 
         RowBox[{"(*", 
          RowBox[{"Plots", " ", "our", " ", "graph"}], "*)"}], 
         RowBox[{"PlotRange", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"fun", "[", "lower", "]"}], "-", "1"}], ",", 
            RowBox[{
             RowBox[{"fun", "[", "upper", "]"}], "+", "1"}]}], "}"}]}]}], 
        "]"}], ",", 
       RowBox[{"Graphics", "[", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Rectangle", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"i", "+", "c"}], ",", "0"}], "}"}], ",", 
              RowBox[{"(*", 
               RowBox[{
               "Prepares", " ", "our", " ", "table", " ", "of", " ", 
                "rectangles"}], "*)"}], 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"If", "[", 
                 RowBox[{
                  RowBox[{"IntervalMemberQ", "[", 
                   RowBox[{
                    RowBox[{"Interval", "[", 
                    RowBox[{"{", 
                    RowBox[{"i", ",", 
                    RowBox[{"i", "+", "c"}]}], "}"}], "]"}], ",", "root"}], 
                   "]"}], ",", "0", ",", 
                  RowBox[{"(*", 
                   RowBox[{
                   "Catches", " ", "negative", " ", "values", " ", "and", " ",
                     "zeros"}], "*)"}], 
                  RowBox[{"If", "[", 
