Cell[BoxData[
 RowBox[{
  RowBox[{"rectanglesToFun", "[", 
   RowBox[{"fun_", ",", 
    RowBox[{"{", 
     RowBox[{"lower_", ",", "upper_"}], "}"}], ",", "p_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"c", "=", 
      FractionBox[
       RowBox[{"(", 
        RowBox[{"upper", "-", "lower"}], ")"}], "p"]}], "}"}], ",", 
    RowBox[{"(*", 
     RowBox[{"creates", " ", "the", " ", "increment"}], "*)"}], 
    RowBox[{"Show", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Plot", "[", 
        RowBox[{
         RowBox[{"fun", "[", "x", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"lower", "-", 
            RowBox[{"2", "c"}]}], ",", 
           RowBox[{"upper", "+", 
            RowBox[{"2", "c"}]}]}], "}"}], ",", 
         RowBox[{"(*", 
          RowBox[{"Plots", " ", "our", " ", "graph"}], "*)"}], 
         RowBox[{"PlotRange", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"fun", "[", "lower", "]"}], "-", "1"}], ",", 
            RowBox[{
             RowBox[{"fun", "[", "upper", "]"}], "+", "1"}]}], "}"}]}]}], 
        "]"}], ",", 
       RowBox[{"Graphics", "[", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Rectangle", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"i", "+", "c"}], ",", "0"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", 
                RowBox[{"fun", "[", "i", "]"}]}], "}"}]}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "lower", ",", 
              RowBox[{"upper", "-", "c"}], ",", "c"}], "}"}]}], "]"}], ",", 
          RowBox[{"(*", 
           RowBox[{
           "Prepares", " ", "our", " ", "table", " ", "of", " ", 
            "rectangles"}], "*)"}], 
          RowBox[{"Directive", "[", 
           RowBox[{
            RowBox[{"EdgeForm", "[", 
             RowBox[{"{", 
              RowBox[{"Thick", ",", "Red"}], "}"}], "]"}], ",", "White"}], 
           "]"}]}], "]"}], "]"}]}], "}"}], "]"}]}], "]"}]}]], "Input",
