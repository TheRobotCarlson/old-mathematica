Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "This", " ", "code", " ", "uses", " ", "the", " ", "uniform", " ", 
    "distribution", " ", "function", " ", "to", " ", "determine", " ", "the", 
    " ", "probability", " ", "of", " ", "any", " ", "sum", " ", "occuring", 
    " ", "where", " ", "each", " ", "dice", " ", "has", " ", "the", " ", 
    "values", " ", "1", " ", "though", " ", "6."}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"Grid", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Range", "[", 
        RowBox[{"2", ",", "12"}], "]"}], ",", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Probability", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Dice1", " ", "+", " ", "Dice2"}], " ", "\[Equal]", " ", 
            "sum"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{
