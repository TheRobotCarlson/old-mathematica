           RowBox[{"gameStats", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "  ", "\[Equal]", " ", 
           "utaPick"}], ",", 
          RowBox[{"utaCounter", "++"}], ",", 
          RowBox[{"clausCounter", "++"}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"totalCount", " ", "+=", " ", 
         RowBox[{"gameStats", "[", 
          RowBox[{"[", "3", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"gameStats", " ", "=", " ", 
         RowBox[{"uDiceGame", "[", 
          RowBox[{"utaPick", ",", "clausPick"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"count", "++"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"N", "[", 
